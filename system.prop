#
# system.prop for espresso
#
rild.libpath=/system/lib/libsec-ril.so
rild.libargs=-d /dev/ttys0
telephony.lteOnCdmaDevice=0
ro.ril.hsxpa=1
ro.ril.gprsclass=10
ro.sf.hwrotation=270

# Disable GC freed lines
dalvik.vm.debug.alloc=0

ro.device.cpu=Dual-core 1 GHz Cortex-A9
ro.device.gpu=PowerVR SGX540
ro.device.rear_cam=3.1 MP
ro.device.front_cam=0.3 MP
ro.device.screen_res=7.0 inch
